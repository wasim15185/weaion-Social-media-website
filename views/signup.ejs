    <!DOCTYPE html>
<html lang="en">
<head>
	<title>Weion Login</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
<!--===============================================================================================-->	
	<link rel="icon" type="image/png" href="/suvlogin/images/icons/favicon.ico"/>
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/bootstrap/css/bootstrap.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/fonts/iconic/css/material-design-iconic-font.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/animate/animate.css">
<!--===============================================================================================-->	
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/css-hamburgers/hamburgers.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/animsition/css/animsition.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/select2/select2.min.css">
<!--===============================================================================================-->	
	<link rel="stylesheet" type="text/css" href="/suvlogin/vendor/daterangepicker/daterangepicker.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/suvlogin/css/util.css">
	<link rel="stylesheet" type="text/css" href="/suvlogin/css/main.css">
<!--===============================================================================================-->
</head>
<body>
	
<%if(msg){ %>
	<div class="alert alert-danger forErr-js" role="alert">
	<%=msg %>
	</div>
<%} %>
	
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-t-75 p-b-20">
				<form class="login100-form validate-form" method="POST" action="/signup">
					<span class="login100-form-title p-b-60">
						Registration
					</span>
					<span class="login100-form-avatar">
						<a href="/" style="cursor: pointer;"><img src="/suvlogin/images/logo1.jpg" alt="AVATAR"></a>
					</span>

					<div class="wrap-input100 validate-input m-t-60 m-b-35" data-validate = "Enter username">
						<input class="input100" type="text" name="name">
						<span class="focus-input100" data-placeholder="Name"></span>
					</div>

					<div class="wrap-input100 validate-input m-b-50" data-validate="Enter gmail">
						<input class="input100" type="email" name="email">
						<span class="focus-input100" data-placeholder="Gmail"></span>
					</div>

					<div class="wrap-input100 validate-input m-b-50" data-validate="Enter password">
						<input class="input100" type="password" name="password">
						<span class="focus-input100" data-placeholder="Password"></span>
					</div>

					<div class="container-login100-form-btn">
						<button class="login100-form-btn">
							Register
						</button>
					</div>

					<ul class="login-more p-t-60">
						

						<li>
							<span class="txt1">
								Already signd up !!! 
							</span>

							<a href="/signin" class="txt2">
								Sign In
							</a>
						</li>
						<li>
							<span class="txt1">
								Goto Homepage    
							</span>

							<a href="/" class="txt2">
								Click 
							</a>
						</li>
					</ul>
				</form>
			</div>
		</div>
	</div>
	

	<div id="dropDownSelect1"></div>
	
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/animsition/js/animsition.min.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/bootstrap/js/popper.js"></script>
	<script src="/suvlogin/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/daterangepicker/moment.min.js"></script>
	<script src="/suvlogin/vendor/daterangepicker/daterangepicker.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/vendor/countdowntime/countdowntime.js"></script>
<!--===============================================================================================-->
	<script src="/suvlogin/js/main.js"></script>
<!--===============================================================================================-->

<script>



setTimeout(()=>{
	hideAlertDiv()
},5000)

function hideAlertDiv(){
	$('.forErr-js').slideUp()
}


</script>

</body>
</html>